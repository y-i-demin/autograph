.contacts
  font-size: 18px

  width: 912px
  padding: 0 6px 30px
  margin: 0 auto
  
  border-bottom: 6px solid
  color: #273440

  &_inner
    border-top: 1px dashed #B98858
    padding-top: 30px

.socials,
.address
  margin-right: 50px
  float: left

  clearfix()

.social
  display: block
  width: 25px
  height: @width

  float: left

  background-repeat: no-repeat
  background-position: left center
  background-image: url("../i/icons-social.png")
  background-image: url("data:image/png;base64,")

  &_fb
    background-position: -33px
    margin-left: 8px

.phone
  font-size: 21px

.map
  position: absolute
  bottom: -115px
  left: 0

  width: 100%
  height: 100%
