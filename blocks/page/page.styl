.pages
  width: 100%
  height: 100%
  
  min-height: 660px
  overflow: hidden

.page
  @extend .pages

  z-index: 1
  position: relative

  &::before
    display: block
    height: 215px
    content: " "

  &_content
    position: relative
    height: 100%

  &_services,
  &_contacts
    background-color: #fff

  &_portfolio
    background-color: #364654

.paranja
  z-index: 10
  fixed top bottom left right

  display: none

  background: rgba(0, 0, 0, .62)

